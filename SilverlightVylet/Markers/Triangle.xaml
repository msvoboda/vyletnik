<UserControl x:Class="CustomMarkers.Triangle"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="14" Width="10">
    <Grid>
        <Polygon
    Points="0,0 5,14 10,0 0,0"
    Stroke="Black" 
    StrokeThickness="0.3">
            <Polygon.Fill>
                <SolidColorBrush Color="Blue" Opacity="0.65"/>
            </Polygon.Fill>
        </Polygon>
    </Grid>
</UserControl>
